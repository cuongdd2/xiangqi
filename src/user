create database xiangqi;

CREATE TABLE user(
  `id` INT(5) NOT NULL AUTO_INCREMENT,
  `name` CHAR(10) NOT NULL DEFAULT '',
  `password` CHAR(10) NOT NULL DEFAULT '',
  `winRate` INT (4) NOT NULL DEFAULT 0,
  `eleResult` INT(8) NOT NULL DEFAULT 0,
  PRIMARY KEY (`id`)
);


